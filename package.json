{
  "name": "gitlab-time-tracker",
  "version": "1.8.0",
  "type": "module",
  "description": "A command line interface for GitLabs time tracking feature.",
  "bugs": {
    "url": "https://github.com/ndu2/gitlab-time-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndu2/gitlab-time-tracker.git"
  },
  "main": "src/gtt.js",
  "scripts": {
    "test": "NODE_ENV=test mocha 'spec/**/*.spec.js'",
    "cover": "istanbul cover _mocha $(find ./spec -name \"*.spec.js\")",
    "coveralls": "yarn run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "build": "pkg src/gtt.js -o out/gtt -c package.json"
  },
  "bin": {
    "gtt": "src/gtt.js"
  },
  "engines": {
    "node": ">=20"
  },
  "author": "kriskbx",
  "license": "GPL-2.0",
  "dependencies": {
    "async": "^3.2.6",
    "camelcase": "^8.0.0",
    "cli-cursor": "^5.0.0",
    "cli-table": "^0.3.11",
    "colors": "^1.4.0",
    "commander": "^14.0.0",
    "csv-string": "^4.1.1",
    "find-in-files": "^0.5.0",
    "hash-sum": "^2.0.0",
    "hashids": "^2.3.0",
    "inquirer": "^12.6.1",
    "markdown-table": "^3.0.4",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "node-spinner": "^0.0.4",
    "open": "^10.1.2",
    "progress": "^2.0.3",
    "prompt": "^1.3.0",
    "read-yaml": "^1.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "shelljs": "^0.10.0",
    "swissqrbill": "^4.1.2",
    "tempfile": "^5.0.0",
    "throttled-queue": "^2.1.4",
    "underscore": "^1.13.7",
    "xdg-basedir": "^5.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "coveralls": "^3.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^11.4.0",
    "mocha-lcov-reporter": "^1.3.0",
    "pkg": "^5.8.1",
    "sinon": "^20.0.0"
  }
}
